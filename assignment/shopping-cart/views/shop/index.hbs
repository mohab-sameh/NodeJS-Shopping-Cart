
<h1>{{title}}</h1>


{{# each products }}
    <div class="row">
        {{# each this }}
            <div class="col-sm-4 co l-md-4 box">
                <div class="thumbnail box">
                    <img src=" {{#each this}} {{ imagePath }} {{/each}} " alt="..." class ="img-responsive">
                    <div class="caption">
                        <h3> {{{title}}} </h3>
                        <p class="description"> {{#each this}} {{ description }} {{/each}} </p>
                        <div class="clearfix">
                            <div class ="price "> ${{#each this}} {{ price }} {{/each}} </div>
                            <a href="#" class="btn btn-default pull-right" role="button">Add to Cart</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
{{/each}}